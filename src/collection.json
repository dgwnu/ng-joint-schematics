{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "element": {
      "description": "ng-joint shape element schematics",
      "factory": "./shape/index#ngJointElementSchematics",
      "schema": "./shape/shape-schema.json"
    },
    "link": {
      "description": "ng-joint shape link schematics",
      "factory": "./shape/index#ngJointLinkSchematics",
      "schema": "./shapes/shape-schema.json"
    },
    "build": {
      "description": "ng-joint build schematics",
      "factory": "./build/index#ngJointBuildSchematics",
      "schema": "./build/build-schema.json"
    }

  }
}
