{
  "name": "@dgwnu/ng-joint-schematics",
  "version": "0.0.1",
  "description": "Angular Schematics to support ng-joint development",
  "scripts": {
    "start": "nodemon",
    "build": "npm run build:schemas && npm run build:schematics",
    "build:schemas": "ts-node -P cfg/tsconfig.json cfg/build-schemas.ts",
    "build:schematics": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "test:sync:ng:cli": "ts-node src/ng-joint-schematics/build/build-utils.ts"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^7.3.1",
    "@angular-devkit/schematics": "^7.3.1",
    "@angular/cli": "^7.3.1",
    "@types/jasmine": "^3.0.0",
    "fs-rmdir-recursive": "^1.0.0",
    "jasmine": "^3.0.0",
    "typescript": "~3.2.2"
  },
  "devDependencies": {
    "@types/node": "^8.10.40",
    "@types/npm": "^2.0.29",
    "json-schema-to-typescript": "^6.1.1",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2"
  }
}
